{
  "name": "expensetracker",
  "license": "0BSD",
  "version": "1.2.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "node scripts/inject-version.js && expo run:android --device",
    "build:ios": "node scripts/inject-version.js && expo run:ios --device",
    "prebuild": "expo prebuild --clean",
    "inject-version": "node scripts/inject-version.js",
    "version:current": "node scripts/version-manager.js current",
    "version:patch": "node scripts/version-manager.js patch && node scripts/inject-version.js",
    "version:minor": "node scripts/version-manager.js minor && node scripts/inject-version.js",
    "version:major": "node scripts/version-manager.js major && node scripts/inject-version.js",
    "version:set": "node scripts/version-manager.js set"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "^8.2.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "expo": "~52.0.47",
    "expo-application": "~6.0.2",
    "expo-calendar": "~14.0.6",
    "expo-dev-client": "~5.0.20",
    "expo-file-system": "~18.0.12",
    "expo-haptics": "~14.0.1",
    "expo-intent-launcher": "~12.0.2",
    "expo-linear-gradient": "~14.0.2",
    "expo-modules-core": "~2.2.3",
    "expo-notifications": "~0.29.14",
    "expo-status-bar": "~2.0.1",
    "lucide-react-native": "^0.525.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-toast-message": "^2.3.2",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.13",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^18.0.0",
    "@react-native/metro-config": "^0.80.1"
  },
  "private": true
}

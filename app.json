{
  "expo": {
    "name": "ExpenseTracker",
    "slug": "ExpenseTracker",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/Exp_Tracker.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Exp_Tracker.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "background-processing",
          "background-fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Exp_Tracker.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mohit0102.ExpenseTracker",
      "permissions": [
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.USE_EXACT_ALARM"
      ]
    },
    "notification": {
      "icon": "./assets/Exp_Tracker.png",
      "color": "#007bff",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "ðŸ’° Expense Tracker"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/Exp_Tracker.png",
          "color": "#007bff",
          "mode": "production"
        }
      ]
    ],
    "web": {
      "Exp_Tracker": "./assets/Exp_Tracker.png"
    },
    "extra": {
      "eas": {
        "projectId": "9d4ba390-1070-459c-812a-b49dabc2c8eb"
      }
    }
  }
}
